	if level.name() == "l05_bar" then
		bar_assault()
	end

	if level.name() == "l06_rostok" then
		news_bar_assault()
		ros_zasada_spawn()
	end

	if level.name() == "l08_yantar" then
		news_yan_saharov_assault()
	end
end

