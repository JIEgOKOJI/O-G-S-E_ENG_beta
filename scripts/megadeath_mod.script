function bind_stalker()
	if level.name() == "l02_garbage" then
		news_gar_dolg_assault()
	end

	if level.name() == "l03u_agr_underground" then
		kolodets_zaminirovan()
	end

	if level.name() == "l04u_labX18" then
		nashol_trupy_x18()
